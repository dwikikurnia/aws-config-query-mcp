# AWS Configuration
# Replace with your actual AWS credentials or use AWS IAM Role
AWS_REGION=your_aws_region
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_SESSION_TOKEN=your_aws_session_token

# AWS Config Aggregator Name
# Name of your AWS Config Configuration Aggregator
CONFIG_AGGREGATOR_NAME=your_config_aggregator_name

# OpenAI API Configuration
# Get your API key from https://platform.openai.com/account/api-keys
OPENAI_API_KEY=your_openai_api_key

# Grafana API Configuration
# URL of your Grafana instance
GRAFANA_API_URL=https://your-grafana-instance.com/api/ds/query
# API Key from Grafana (Admin > API Keys)
GRAFANA_API_KEY=your_grafana_api_key
